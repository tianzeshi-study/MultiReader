{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
        "event:default",
        "window:default",
        "app:default",
        "resources:default",
        "menu:default",
        "tray:default",
        "window:allow-set-title",
        "dialog:allow-open",
        "dialog:allow-save"
        ]
}
